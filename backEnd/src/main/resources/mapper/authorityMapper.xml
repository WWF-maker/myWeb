<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0
                        //EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.AuthorityMapper">
    <select id="getAllUserPos" resultType="com.example.demo.model.UserPo">
        SELECT * FROM user;
    </select>
    <select id="getPasswordByUserName" resultType="String">
        SELECT password from user where name=#{userName};
    </select>
</mapper>